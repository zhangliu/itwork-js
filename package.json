{
    "name": "itwork-js",
    "displayName": "itwork-js",
    "description": "",
    "version": "1.0.0",
    "publisher": "zhangliu",
    "repository": "https://github.com/zhangliu/itwork-js",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.toRun"
    ],
    "main": "./out/extension",
    "contributes": {
        "keybindings": [
            {
                "command": "extension.toRun",
                "describe": "运行选中的方法",
                "key": "ctrl+e",
                "mac": "cmd+e"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@babel/parser": "^7.0.0-beta.48",
        "moment": "^2.22.1"
    }
}